-main ApplicationMain 
-cp /usr/local/lib/haxelib/openfl/3,6,1
-D openfl=3.6.1
-cp /usr/local/lib/haxelib/lime/2,9,1
-D lime=2.9.1
-cp src
-D openfl-next
-D tools=2.9.1
-D no-compilation
-D lime-opengl
-D openfl-html5
-D canvas
-D lime-html5
-D html5
-D web
--remap flash:openfl
-js bin/html5/bin/drawingimages.js
-cp bin/html5/haxe
-D html5
--macro allowPackage("flash")
-dce full
-D final
-D js-flatten
-dce full